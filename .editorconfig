[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true
max_line_length = 150

# JavaScript
[*.{jsx,js,ts,tsx}]
indent_style = space
indent_size = 3

# JSON
[*.json]
indent_style = space
indent_size = 3

# STYLES
[*.{css,scss}]
indent_style = space
indent_size = 3
